{
  "name": "d3-bundle-element",
  "version": "1.0.1",
  "description": "polymer element for importing d3.js (v4) as optimized bundle",
  "keywords": [
    "web-component",
    "polymer",
    "indicator",
    "d3",
    "d3-bundle"
  ],
    "authors": [
    "christophe-g <christophe.geiser@gmail.com>"
  ],
  "homepage": "https://github.com/PolymerEl/d3-bundle-element",
  "main": "d3-bundle-element.html",
  "license": "MIT",
  "homepage": "https://github.com/polymerEl/d3-bundle-element",
  "scripts": {
    "prepublish": "rollup -c && rollup -c rollup.shape.config.js && rollup -c rollup.multi.config.js && rollup -c rollup.small.config.js && uglifyjs d3.js -c -m -o d3.min.js && uglifyjs d3-shape.js -c -m -o d3-shape.min.js  && uglifyjs d3-multi.js -c -m -o d3-multi.min.js && uglifyjs d3-small.js -c -m -o d3-small.min.js"
  },
  "dependencies": {
    "d3-array": "1.0.1",
    "d3-axis": "1.0.3",
    "d3-brush": "1.0.3",
    "d3-chord": "1.0.2",
    "d3-collection": "1.0.1",
    "d3-color": "1.0.1",
    "d3-dispatch": "1.0.1",
    "d3-drag": "1.0.1",
    "d3-dsv": "1.0.3",
    "d3-ease": "1.0.1",
    "d3-force": "1.0.2",
    "d3-format": "1.0.2",
    "d3-geo": "1.2.5",
    "d3-hierarchy": "1.0.2",
    "d3-interpolate": "1.1.1",
    "d3-path": "1.0.2",
    "d3-polygon": "1.0.1",
    "d3-quadtree": "1.0.1",
    "d3-queue": "3.0.3",
    "d3-random": "1.0.1",
    "d3-request": "1.0.2",
    "d3-scale": "1.0.3",
    "d3-selection": "1.0.2",
    "d3-shape": "1.0.3",
    "d3-time": "1.0.4",
    "d3-time-format": "2.0.2",
    "d3-timer": "1.0.3",
    "d3-transition": "1.0.2",
    "d3-voronoi": "1.0.2",
    "d3-zoom": "1.0.3",
    "d3-selection-multi": "^1.0.0",
    "d3-svg-legend": "^2.9.0"
  },
  "devDependencies": {
    "json2module": "0.0",
    "package-preamble": "0.0",
    "rimraf": "2",
    "rollup": "0.36",
    "rollup-plugin-ascii": "0.0",
    "rollup-plugin-node-resolve": "2",
    "tape": "4",
    "uglify-js": "2",
    "rollup-plugin-node-resolve": "^1.5.0",
    "uglify-js": "^2.6.2"
  }
}
